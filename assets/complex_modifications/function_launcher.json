{
  "title": "Fn Apps Launcher",
  "rules": [
    {
      "description": "[FnLauncher] Fn+Spacebar -> Xcode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": "fn"
            }
          },
          "to": [
            {
              "shell_command": "open -a Xcode"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "fn",
                "spacebar"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a Xcode"
            }
          ]
        }
      ]
    },
    {
      "description": "[FnLauncher] Fn+A-Z -> Apps",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_control"
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "to_if_alone": [
            {
              "shell_command": "open -a LaunchBar"
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 250
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": "fn"
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Arc'"
