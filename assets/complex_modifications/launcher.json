{
  "title": "App Launcher",
  "rules": [{
    "description": "Launch Applications using Fn Key",
    "manipulators": [
      {
        "type": "basic",
        "from": {
          "key_code": "e",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open -a 'Microsoft Edge'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "f",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open -a Finder"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "g",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Fork.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "h",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Hammerspoon.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "i",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Xcode.app/Contents/Instruments.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "k",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Keynote.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "l",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Lark.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "m",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Utilities/Activity Monitor.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "n",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Notion.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "n",
          "modifiers": {
            "mandatory": ["fn", "left_shift"]
          }
        },
        "to": [{
            "shell_command": "open 'x-launchbar:select?abbreviation=AN' && osascript -e 'tell application \"System Events\" to key code 36'"
          }
        ]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "o",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Microsoft OneNote.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "p",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Pixelmator.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "r",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/NetNewsWire.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "t",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Typora.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "u",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/WeChat.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "v",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/IINA.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "x",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/Visual Studio Code.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "z",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/2Do.app'"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "spacebar",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open -a Xcode"
        }]
      },
      {
        "type": "basic",
        "from": {
          "key_code": "comma",
          "modifiers": {
            "mandatory": "fn"
          }
        },
        "to": [{
          "shell_command": "open '/Applications/System Preferences.app'"
        }]
      }
    ]
  }]
}
